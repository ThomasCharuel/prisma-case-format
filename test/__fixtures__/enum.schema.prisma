datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model posts {
  id       Int       @id @default(autoincrement())
  content  String?   @db.VarChar(245)
  postType post_type
}

enum post_type {
  Note
  Question
}

enum snaggle_flark {
  foo
  buz
  bazz
}

enum AppleColors {
  green
  red
}
